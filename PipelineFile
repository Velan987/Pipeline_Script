pipeline {
    agent any
    stages {
		stage('Git-Checkout') {
			steps{
				git 'https://github.com/simplilearn-github/Pipeline_Script.git'
			}
		}
		
		stage('Build') {
			steps{
				bat 'Build.bat'
			}
		}
    stage('Unit-Testing') {
      steps{
        echo "Unit-Testing executing in Windows_Slave"
        bat 'Unit.bat'
      }

    stage('Quality-Test') {
      steps{
        echo "Quality-Testing executing in master"
        bat 'Quality.bat'
      }
    }
		stage('Deploy') {
			steps{
				bat 'Deploy.bat'
			}
		}
    }
}
